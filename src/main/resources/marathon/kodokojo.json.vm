{
  "id": "$ID",
  "instance": 1,
  "cpu": 0.1,
  "mem": 128,
  "type": "DOCKER",
  "container": {
    "docker": {
      "image": "kodokojo/kodokojo",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 0,
          "protocol": "tcp",
          "servicePort": 0
        }
      ],
      "parameters": [
        { "key": "env", "value": "redis.host=$REDISHOST" },
        { "key": "env", "value": "redis.port=$REDISPORT" },
        {
          "key": "log-opt",
          "value": "tag=kodokojo-back"
        }
      ]
    }
  },

  "labels": {
    "entity": "kodokojo",
    "component": "back"
  }
}